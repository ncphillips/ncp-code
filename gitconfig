[alias]
  s = status
  c = commit
  co = checkout
  amend = commit --amend
  aa = add --all

  shred  = "!source ~/ncp-code/githelpers && shred_git_branches"

  l = "!source ~/ncp-code/githelpers && pretty_git_log"
  la = !git l --all
  r = !GIT_NO_PAGER=1 git l -30
  ra = !git r --all
  b = "!source ~/ncp-code/githelpers && pretty_git_branch"
